{
    "cpu": {
        "name": "Processeur",
        "score-formula": "[cpu-cores]*[cpu-frequency]+[cpu-boost-frequency]+[cpu-threads]+[cpu-l1-cache]+[cpu-l2-cache]+[cpu-l3-cache]",
        "specs": {
            "cpu-frequency": {
                "input-type": "number",
                "name": "Fréquence",
                "unit": "GHz"
            },
            "cpu-boost-frequency": {
                "input-type": "number",
                "name": "Fréquence boost",
                "unit": "GHz"
            },
            "cpu-l1-cache": {
                "input-type": "number",
                "name": "Cache L1",
                "unit": "Mo"
            },
            "cpu-l2-cache": {
                "input-type": "number",
                "name": "Cache L2",
                "unit": "Mo"
            },
            "cpu-l3-cache": {
                "input-type": "number",
                "name": "Cache L3",
                "unit": "Mo"
            },
            "cpu-cores": {
                "input-type": "number",
                "name": "Coeurs physiques"
            },
            "cpu-threads": {
                "input-type": "number",
                "name": "Coeurs logiques"
            },
            "cpu-ram-max-frequency": {
                "input-type": "number",
                "name": "Fréquence max. de la mémoire",
                "unit": "MHz"
            },
            "cpu-socket": {
                "input-type": "text",
                "name": "Socket"
            },
            "cpu-cmos": {
                "input-type": "number",
                "name": "Finesse de gravure",
                "unit": "nm"
            }
        }
    },
    "gpu": {
        "name": "Carte graphique",
        "score-formula": "([gpu-frequency]+[gpu-boost-frequency])/10+[gpu-memory-capacity]*10+[gpu-memory-frequency]/100+[gpu-flux-processors]/10",
        "specs": {
            "gpu-frequency": {
                "input-type": "number",
                "name": "Fréquence",
                "unit": "MHz"
            },
            "gpu-boost-frequency": {
                "input-type": "number",
                "name": "Fréquence boost",
                "unit": "MHz"
            },
            "gpu-memory-type": {
                "input-type": "text",
                "name": "Type de la mémoire"
            },
            "gpu-memory-capacity": {
                "input-type": "number",
                "name": "Taille de la mémoire",
                "unit": "Go"
            },
            "gpu-memory-frequency": {
                "input-type": "number",
                "name": "Fréquence de la mémoire",
                "unit": "MHz"
            },
            "gpu-flux-processors": {
                "input-type": "number",
                "name": "Processeurs de flux"
            },
            "gpu-cmos": {
                "input-type": "number",
                "name": "Finesse de gravure",
                "unit": "nm"
            }
        }
    },
    "ram": {
        "name": "Mémoire vive",
        "score-formula": "([ram-modules]*[ram-capacity])+([ram-frequency]/100)+(-0.5*[ram-cas-latency]+20)",
        "specs": {
            "ram-type": {
                "input-type": "text",
                "name": "Type"
            },
            "ram-modules": {
                "input-type": "number",
                "name": "Barrettes mémoire"
            },
            "ram-capacity": {
                "input-type": "number",
                "name": "Taille de la mémoire",
                "unit": "Go"
            },
            "ram-frequency": {
                "input-type": "number",
                "name": "Fréquence de la mémoire",
                "unit": "MHz"
            },
            "ram-cas-latency": {
                "input-type": "number",
                "name": "CAS Latency",
                "unit": "CL"
            }
        }
    },
    "hdd": {
        "name": "Disque dur",
        "score-formula": "([hdd-speed]/100)+[hdd-cache]+([hdd-capacity]/100)",
        "specs": {
            "hdd-speed": {
                "input-type": "number",
                "name": "Vitesse de rotation",
                "unit": "tr/min"
            },
            "hdd-capacity": {
                "input-type": "number",
                "name": "Capacité",
                "unit": "Go"
            },
            "hdd-cache": {
                "input-type": "number",
                "name": "Cache",
                "unit": "Mo"
            },
            "hdd-form-factor": {
                "input-type": "select",
                "name": "Format",
                "values": {
                    "3.5": "3.5 pouces",
                    "2.5": "2.5 pouces"
                }
            }
        }
    },
    "ssd": {
        "name": "SSD",
        "score-formula": "(log([ssd-capacity]+1)*10)+[ssd-write-speed]/100+[ssd-read-speed]/100",
        "specs": {
            "ssd-type": {
                "input-type": "select",
                "name": "Type",
                "values": {
                    "nvme": "M.2 NVMe",
                    "sata": "SATA",
                    "msata": "M.2 SATA"
                }
            },
            "ssd-capacity": {
                "input-type": "number",
                "name": "Capacité",
                "unit": "Go"
            },
            "ssd-read-speed": {
                "input-type": "number",
                "name": "Vitesse de lecture",
                "unit": "Mo/s"
            },
            "ssd-write-speed": {
                "input-type": "number",
                "name": "Vitesse d'écriture",
                "unit": "Mo/s"
            }
        }
    },
    "mb": {
        "name": "Carte mère",
        "score-formula": "[mb-pcie]+[mb-wifi]+[mb-sata-slots]+[mb-m2-slots]",
        "specs": {
            "mb-form-factor": {
                "input-type": "select",
                "name": "Format",
                "values": {
                    "eatx": "EATX",
                    "atx": "ATX Standard",
                    "matx": "Micro-ATX",
                    "itx": "Mini-ITX"
                }
            },
            "mb-chipset": {
                "input-type": "text",
                "name": "Chipset"
            },
            "mb-socket": {
                "input-type": "text",
                "name": "Socket"
            },
            "mb-wifi": {
                "input-type": "select",
                "name": "WiFi",
                "values": {
                    "yes": "Oui",
                    "no": "Non"
                }
            },
            "mb-pcie": {
                "input-type": "number",
                "name": "Version du PCIe"
            },
            "mb-sata-slots": {
                "input-type": "number",
                "name": "Quantitié d'emplacements SATA"
            },
            "mb-m2-slots": {
                "input-type": "number",
                "name": "Quantité d'emplacements M.2"
            }
        }
    },
    "ps": {
        "name": "Alimentation",
        "score-formula": "[ps-wattage]/100",
        "specs": {
            "ps-wattage": {
                "input-type": "number",
                "name": "Puissance",
                "unit": "W"
            },
            "ps-modular": {
                "input-type": "select",
                "name": "Modulaire",
                "values": {
                    "yes": "Oui",
                    "no": "Non"
                }
            }
        }
    }
}