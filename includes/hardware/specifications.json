{
    "cpu": {
        "name": "Processeur",
        "score-formula": "([cpu-cores]+[cpu-threads])*([cpu-frequency]+[cpu-boost-frequency])+(1/[cpu-cmos]*100)+(cpu-l1-cache+cpu-l2-cache+cpu-l3-cache)",
        "specs": {
            "cpu-frequency": {
                "input-type": "number",
                "name": "Fréquence",
                "unit": "GHz"
            },
            "cpu-boost-frequency": {
                "input-type": "number",
                "name": "Fréquence boost",
                "unit": "GHz"
            },
            "cpu-l1-cache": {
                "input-type": "number",
                "name": "Cache L1",
                "unit": "Mo"
            },
            "cpu-l2-cache": {
                "input-type": "number",
                "name": "Cache L2",
                "unit": "Mo"
            },
            "cpu-l3-cache": {
                "input-type": "number",
                "name": "Cache L3",
                "unit": "Mo"
            },
            "cpu-cores": {
                "input-type": "number",
                "name": "Coeurs physiques"
            },
            "cpu-threads": {
                "input-type": "number",
                "name": "Coeurs logiques"
            },
            "cpu-ram-max-frequency": {
                "input-type": "number",
                "name": "Fréquence max. de la mémoire",
                "unit": "MHz"
            },
            "cpu-socket": {
                "input-type": "text",
                "name": "Socket"
            },
            "cpu-cmos": {
                "input-type": "number",
                "name": "Finesse de gravure",
                "unit": "nm"
            }
        }
    },
    "gpu": {
        "name": "Carte graphique",
        "score-formula": "([gpu-frequency]+[gpu-boost-frequency])+([gpu-memory-capacity]*[gpu-memory-frequency])+[gpu-flux-processors]+(1/[gpu-cmos]*100)",
        "specs": {
            "gpu-frequency": {
                "input-type": "number",
                "name": "Fréquence",
                "unit": "MHz"
            },
            "gpu-boost-frequency": {
                "input-type": "number",
                "name": "Fréquence boost",
                "unit": "MHz"
            },
            "gpu-memory-type": {
                "input-type": "text",
                "name": "Type de la mémoire"
            },
            "gpu-memory-capacity": {
                "input-type": "number",
                "name": "Taille de la mémoire",
                "unit": "Go"
            },
            "gpu-memory-frequency": {
                "input-type": "number",
                "name": "Fréquence de la mémoire",
                "unit": "MHz"
            },
            "gpu-flux-processors": {
                "input-type": "number",
                "name": "Processeurs de flux"
            },
            "gpu-cmos": {
                "input-type": "number",
                "name": "Finesse de gravure",
                "unit": "nm"
            }
        }
    },
    "ram": {
        "name": "Mémoire vive",
        "score-formula": "([ram-modules]*[ram-capacity])*[ram-frequency]",
        "specs": {
            "ram-type": {
                "input-type": "text",
                "name": "Type"
            },
            "ram-modules": {
                "input-type": "number",
                "name": "Barrettes mémoire"
            },
            "ram-capacity": {
                "input-type": "number",
                "name": "Taille de la mémoire",
                "unit": "Go"
            },
            "ram-frequency": {
                "input-type": "number",
                "name": "Fréquence de la mémoire",
                "unit": "MHz"
            },
            "ram-cas-latency": {
                "input-type": "number",
                "name": "CAS Latency",
                "unit": "CL"
            }
        }
    },
    "hdd": {
        "name": "Disque dur",
        "score-formula": "[hdd-speed]*([hdd-cache]*[hdd-capacity])/100000",
        "specs": {
            "hdd-speed": {
                "input-type": "number",
                "name": "Vitesse de rotation",
                "unit": "tr/min"
            },
            "hdd-capacity": {
                "input-type": "number",
                "name": "Capacité",
                "unit": "Go"
            },
            "hdd-cache": {
                "input-type": "number",
                "name": "Cache",
                "unit": "Mo"
            },
            "hdd-form-factor": {
                "input-type": "select",
                "name": "Format",
                "values": {
                    "3.5": "3.5 pouces",
                    "2.5": "2.5 pouces"
                }
            }
        }
    },
    "ssd": {
        "name": "SSD",
        "score-formula": "[ssd-capacity]*[ssd-write-speed]*[ssd-read-speed]/1000000",
        "specs": {
            "ssd-type": {
                "input-type": "select",
                "name": "Type",
                "values": {
                    "nvme": "M.2 NVMe",
                    "sata": "SATA",
                    "msata": "M.2 SATA"
                }
            },
            "ssd-capacity": {
                "input-type": "number",
                "name": "Capacité",
                "unit": "Go"
            },
            "ssd-read-speed": {
                "input-type": "number",
                "name": "Vitesse de lecture",
                "unit": "Mo/s"
            },
            "ssd-write-speed": {
                "input-type": "number",
                "name": "Vitesse d'écriture",
                "unit": "Mo/s"
            }
        }
    },
    "mb": {
        "name": "Carte mère",
        "score-formula": "1",
        "specs": {
            "mb-chipset": {
                "input-type": "text",
                "name": "Chipset"
            },
            "mb-socket": {
                "input-type": "text",
                "name": "Socket"
            },
            "mb-wifi": {
                "input-type": "select",
                "name": "WiFi",
                "values": {
                    "yes": "Oui",
                    "no": "Non"
                }
            },
            "mb-pcie": {
                "input-type": "number",
                "name": "Version du PCIe"
            }
        }
    }
}